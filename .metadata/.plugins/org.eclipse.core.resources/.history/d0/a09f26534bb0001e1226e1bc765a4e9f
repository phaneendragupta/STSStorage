package StreamsExrPkg;

public class Str {
	public static void main(String[] args) {
		String S1="Capgemini";
		String S2="";
		int L1=S1.length();
		for (int i=0;i<L1;i++) {
			if(i%2==0) {
				S2=S2 +S1.charAt(i);
			}
			
			
			else { 
			S2= S2 +"$";
			}
			
			
		}
		
		System.out.println(S2);
		
		
		
	}

}
